{
  "name": "daily-incomer",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "config": "*",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "syncpack": "^9.8.6",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "packageManager": "yarn@3.5.1",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "deps:format": "syncpack format",
    "deps:sync": "syncpack fix-mismatches",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:style": "turbo run lint:style",
    "postinstall": "husky install",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Package manager not supported! Use YARN to install!')\"",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
